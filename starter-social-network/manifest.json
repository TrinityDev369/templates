{
  "name": "starter-social-network",
  "version": "1.0.0",
  "description": "Social network with feeds, profiles, and messaging",
  "outputDir": ".",
  "files": [
    "package.json",
    "tsconfig.json",
    "next.config.ts",
    "tailwind.config.ts",
    "postcss.config.mjs",
    ".env.example",
    "src/app/globals.css",
    "src/app/layout.tsx",
    "src/app/page.tsx",
    "src/app/profile/[username]/page.tsx",
    "src/app/explore/page.tsx",
    "src/app/messages/page.tsx",
    "src/app/api/posts/route.ts",
    "src/app/api/posts/[id]/like/route.ts",
    "src/app/api/users/[id]/follow/route.ts",
    "src/components/post-card.tsx",
    "src/components/follow-button.tsx",
    "src/lib/db.ts",
    "src/lib/schema.sql",
    "src/types/index.ts",
    "src/app/new-post-form.tsx",
    "src/app/messages/message-thread.tsx"
  ],
  "peerDependencies": {
    "react": ">=18",
    "next": ">=14",
    "tailwindcss": ">=3",
    "postgres": ">=3"
